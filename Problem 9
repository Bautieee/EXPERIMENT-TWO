#include <iostream>

using namespace std;

int main ()

//SodiumNitrogenZinc//
{
	
  	 int R,C; 
  	 
  	 
  	 
  	 
  	 	cout << "Number of columns: "; cin >> C;
  		cout << "Number of rows: "; cin >> R;
  		
  		
  		{
  			 	for ( int y = 0; y < C; y++ )
  			 		{
  			 			cout << "\n*";
  			 				for ( int x = 1; x < R; x++ )
  			 					{
  			 						cout << "*";
					  			}
  			 	
			  		}
		}
		  
		cout << "\n\n";
		
	
	 
	 //SodiumNitrogenZinc//
	 return 0;
	 
}
